{
  "manifest_version": 3,
  "name": "Random European IBAN Generator",
  "version": "1.1.0",
  "description": "Random European IBAN Generator â€“ generate valid IBANs for all 44 European countries. Works offline, for testing and educational purposes only.",
  "permissions": [
    "storage",
    "clipboardWrite"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Random IBAN Generator"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["fontawesome/webfonts/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}
  